}

@article{hauck_nitrogen_1981,
	title = {{NITROGEN} {FERTILIZER} {EFFECTS} {ON} {NITROGEN} {CYCLE} {PROCESSES}},
	issn = {0346-6868},
	url = {https://www.jstor.org/stable/45128688},
	abstract = {The participation of nitrogen fertilizers in nitrogen-cycle processes is discussed from two interrelated viewpoints. The first considers fertilizer nitrogen as a major nutrient input into the biosphere, and how it affects crop productivity and the quantitative aspects of nitrogen transfer within the biosphere and between the biosphere and the atmosphere. When considered as chemicals added to the biosphere, nitrogen fertilizers also affect the qualitative aspects of nitrogen-cycle processes, that is the manner in which these processes are changed by the participation of nitrogen fertilizers in them. In this regard, their chemical and physical form, the manner in which they are applied, and the intensity of fertilization are dominant factors which should be considered when evaluating nitrogen-fertilizer effects on nitrogen-transformation processes. The information required for such evaluation is available only for the nitrification process and for ammonia evolution from surface-applied nitrogen fertilizers. Also discussed are the concepts involved in extrapolating to global dimensions data obtained from extremely small areas or volumes over short time intervals.},
	number = {33},
	urldate = {2021-07-29},
	journal = {Ecological Bulletins},
	author = {Hauck, R.D.},
	year = {1981},
	pages = {551--562},
}
