	pmid = {2002850},
	keywords = {Chemical Phenomena, Oxidation-Reduction, Binding Sites, Protein Conformation, Tyrosine, Copper, Fungi, Galactose Oxidase, X-Ray Diffraction, Molecular Structure, Crystallization, Chemistry, Physical, Free Radicals},
	pages = {87--90},
}

