@article{parr_electron-transfer_1977,
	title = {The electron-transfer reaction between azurin and the cytochrome c oxidase from {Pseudomonas} aeruginosa},
	volume = {167},
	issn = {0264-6021},
	doi = {10.1042/bj1670447},
	abstract = {A stopped-flow investigation of the electron-transfer reaction between oxidized azurin and reduced Pseudomonas aeruginosa cytochrome c-551 oxidase and between reduced azurin and oxidized Ps. aeruginosa cytochrome c-551 oxidase was performed. Electrons leave and enter the oxidase molecule via its haem c component, with the oxidation and reduction of the haem d1 occurring by internal electron transfer. The reaction mechanism in both directions is complex. In the direction of oxidase oxidation, two phases assigned on the basis of difference spectra to haem c proceed with rate constants of 3.2 X 10(5)M-1-S-1 and 2.0 X 10(4)M-1-S-1, whereas the haem d1 oxidation occurs at 0.35 +/- 0.1S-1. Addition of CO to the reduced enzyme profoundly modifies the rate of haem c oxidation, with the faster process tending towards a rate limit of 200S-1. Reduction of the oxidase was similarly complex, with a fast haem c phase tending to a rate limit of 120S-1, and a slower phase with a second-order rate of 1.5 X 10(4)M-1-S-1; the internal transfer rate in this direction was o.25 +/- 0.1S-1. These results have been applied to a kinetic model originally developed from temperature-jump studies.},
	language = {eng},
	number = {2},
	journal = {The Biochemical Journal},
	author = {Parr, S. R. and Barber, D. and Greenwood, C. and Brunori, M.},
	month = nov,
