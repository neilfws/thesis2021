	keywords = {Base Sequence, Cloning, Molecular, Escherichia coli, Genes, Bacterial, Plasmids, Transcription, Genetic, Molecular Sequence Data, Genetic Complementation Test, DNA-Binding Proteins, Escherichia coli Proteins, Mutation, Promoter Regions, Genetic, Recombinant Fusion Proteins, Sequence Deletion, Pseudomonas aeruginosa, Restriction Mapping, Pseudomonas putida, DNA-Directed RNA Polymerases, Sequence Homology, Nucleic Acid, beta-Galactosidase, Genetic Markers, Pyrroline Carboxylate Reductases, RNA Polymerase Sigma 54, Sigma Factor},
	pages = {74--80},
}

@article{saraste_cytochrome_1994,
	title = {Cytochrome oxidase evolved by tinkering with denitrification enzymes},
	volume = {341},
	issn = {0014-5793},
	doi = {10.1016/0014-5793(94)80228-9},
	abstract = {The cytochrome bc complex which is encoded by the fixNOPQ operon in Bradyrhizobium japonicum, is the most distant member of the haem-copper cytochrome oxidase family. We have found that its major subunit, FixN, is homologous to the NorB subunit of nitric oxide reductase in a purple bacterium. A second evolutionary link between cytochrome oxidases and denitrification enzymes is the presence of a similar binuclear copper site in cytochrome aa3 (the mitochondrial oxidase) and nitrous oxide reductase. This centre was probably acquired by a primitive FixN-type oxidase, leading to the evolution of the mitochondrial-type oxidase. These links suggest that the oxygen-reducing respiratory chain developed from the anaerobic, denitrifying respiratory system.},
