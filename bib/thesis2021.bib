	pmid = {8245846},
	keywords = {Base Sequence, Genes, Bacterial, Molecular Sequence Data, Nitrite Reductases, Species Specificity, DNA, Bacterial, Pseudomonas, DNA Probes, Antibodies, Bacterial, Seawater, Water Microbiology},
	pages = {2285--2293},
}

