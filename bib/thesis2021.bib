	author = {Robinson, M. K. and Martinkus, K. and Kennelly, P. J. and Timkovich, R.},
	month = sep,
	year = {1979},
	pmid = {226118},
	keywords = {NAD, Nitrite Reductases, Paracoccus denitrificans, Kinetics, Mathematics, NADH, NADPH Oxidoreductases, Methylphenazonium Methosulfate},
	pages = {3921--3926},
}

