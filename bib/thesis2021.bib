	pmid = {7649312},
	keywords = {Binding Sites, Catalase, Circular Dichroism, Protein Conformation, Animals, Liver, Aspergillus niger, Cattle, Micrococcus luteus, X-Ray Diffraction},
	pages = {97--100},
}

